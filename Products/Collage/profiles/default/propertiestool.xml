<?xml version="1.0"?>

<object name="portal_properties" meta_type="Plone Properties Tool">

    <!--
    NavigationTree Properties Sheet

    We dont want page row and column show in the navigation

    IMPORTANT! set properties to purge="False"
    so they dont overwrite but adds
    -->
    <object name="navtree_properties" meta_type="Plone Property Sheet">
        <property name="title">NavigationTree properties</property>
        <property name="metaTypesNotToList" type="lines" purge="False">
            <element value="CollageRow"/>
            <element value="CollageColumn"/>
        </property>
     </object>

    <!--
    Site Properties Sheet
    -->
    <object name="site_properties" meta_type="Plone Property Sheet">
        <property name="allowAnonymousViewAbout" type="boolean">False</property>
        <property name="default_language" type="string">da</property>
        <property name="default_page_types"
                  type="lines"
                  purge="False">
            <element value="Collage"/>
        </property>
    </object>


</object>